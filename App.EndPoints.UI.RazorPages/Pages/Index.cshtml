				</div> *@

			</div>
		</div>
	</div>
</div>
<!-- ======================= Home Banner ======================== -->
<!-- =========================== Listing Category ======================= -->
<section class="gray middle min">
	<div class="container">

		<div class="row">
			<div class="col-xl-12 col-lg-12 col-md-12 col-12">
				<div class="sec_title position-relative text-center mb-5">
					<h6 class="theme-cl">دسته بندی های محبوب</h6>
					<h4 class="ft-bold ff-second">انتخاب دسته بندی انتخابی شما</h4>
				</div>
			</div>
		</div>

		<div class="row align-items-center">

			@foreach(var category in Model.Categories)
			{
				<div class="col-xl-3 col-lg-3 col-md-6 col-sm-6 col-6">
					<div class="Goodup-img-catg-wrap">
						@* <div class="Goodup-catg-city">12 شهر</div> *@
						<div class="Goodup-img-catg-thumb"><a asp-page="Services" asp-route-categoryId="@category.Id"><img src="assets/img/c-4.jpg" class="img-fluid" alt=""></a></div>
						<div class="Goodup-img-catg-caption">
							<h4 class="fs-md mb-0 ft-medium m-catrio">@category.Title</h4>
							<input type="hidden" value="@category.Id" />
							<a asp-page="Services" asp-route-categoryId="@category.Id" class="Goodup-cat-arrow"><i class="lni lni-arrow-left-circle"></i></a>
						</div>
					</div>
				</div>
			}

		</div>

	</div>
</section>
<!-- =========================== Listing Category End ===================== -->
